{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "JobWorkerConfigurations": {
    "WorkerExecutablePath": "JobWorker1",
    "WorkerName": "JobWorker1"
  },
  "ConnectionStrings": {
    "Default": "Server=SERVER_NAME,PORT;Database=DATABASE;User ID=USERNAME;Password=PASSWORD;"
  },
  "QueueOptions": {
    "Host": "HOST",
    "Username": "USERNAME",
    "Password": "PASSWORD",
    "QueueName": "QUEUE_NAME"
  },
  "Serilog": {
    "Using":  [ "Serilog.Sinks.Console"],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  }
}
